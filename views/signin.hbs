<!-- this page sends a post request to sign in to your already existing account -->
<div class="content quick-center">
  <div class="main-content-panel">
    <h2>Sign In</h2>
    <form id="signup" class = "sign-form" method="post" action="/signup">
    <table>
      <tr>
        <td><label for="username">Username</label></td>
        <td><input class="form-element" id="username"></td>
      </tr>
      <tr>
        <td><label for="password">Password</label></td>
        <td><input class="form-element" id="password" type="password"></td>
      </tr>
      <tr>
        <td></td>
        <td>
            <button form="signup" type="submit">Sign in</button>
            <button onclick="back()">Back</button>
        </td>
      </tr>
    </table>
    </form>
  </div>
</div>
<!-- Include scripts here-->
<script src="../public/js/nav.js"></script>
